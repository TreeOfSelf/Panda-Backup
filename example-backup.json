{
	"connection": {
		"ip": "IP HERE",
		"port": 22,
		"username": "USERNAME HERE",
		"password": "PASSWORD HERE"
	},
	"server": {
		"type": "minecraft",
		"name": "test",
		"runs": true,
		"warnBackup": true,
		"crashDetection": true,
		"restartOnBackup": true,
		"command": {
			"start": "java jar server.jar nogui",
			"stop": "stop",
			"say": "say"
		},
		"prerun": [
			"java -jar /usr/local/bin/mcaselector.jar --mode delete --world 'world' --query 'InhabitedTime < 1min'",
			"grep -v '^#' server.properties > server.properties.tmp && mv server.properties.tmp server.properties"
		]
	},
	"backup": {
		"debug": false,
		"time": "04:00",
		"longBackupDay": 1,
		"types": {
			"data_xz": {
				"type": "both",
				"compression": "xz",
				"threaded": true,
				"shortLimit": 7,
				"longLimit": 0,
				"shortFreq": 1,
				"files": "'world' 'world_nether' 'world_the_end'"
			},
			"data_bz2": {
				"type": "both",
				"compression": "bz2",
				"threaded": false,
				"shortLimit": 7,
				"longLimit": 0,
				"shortFreq": 1,
				"files": "'world' 'world_nether' 'world_the_end'"
			}
		}
	}
}
